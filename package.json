{
  "name": "vuesalize",
  "version": "1.0.3",
  "description": "Component library dedicated to simplifying interactive visualization building in Vue.js 3.",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build && rm dist/favicon.ico",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "docs:dev": "vuepress dev documentation_src",
    "docs:build": "vuepress build documentation_src"
  },
  "main": "./dist/vuesalize.umd.js",
  "module": "./dist/vuesalize.es.js",
  "unpkg": "./dist/vuesalize.umd.js",
  "exports": {
    ".": {
      "import": "./dist/vuesalize.es.js",
      "require": "./dist/vuesalize.umd.js"
    },
    "./dist/vuesalize.css": "./dist/vuesalize.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/haroutboujakjian/Vuesalize"
  },
  "author": "Harout Boujakjian",
  "license": "MIT",
  "dependencies": {
    "d3-array": "^2.11.0",
    "d3-axis": "^2.0.0",
    "d3-force": "^2.1.1",
    "d3-scale": "^3.2.3",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.0.0",
    "d3-transition": "^2.0.0",
    "d3-zoom": "^2.0.0"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vuepress/plugin-register-components": "^2.0.0-beta.60",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "prettier": "^2.7.1",
    "vite": "^4.0.0",
    "vuepress": "^2.0.0-beta.60"
  },
  "keywords": [
    "vue.js",
    "vue",
    "d3",
    "d3.js",
    "visualization",
    "data visualization",
    "component library",
    "ui library"
  ]
}
