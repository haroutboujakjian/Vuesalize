{
  "name": "vue-charts",
  "version": "0.0.1",
  "description": "This directory contains chart components and other basic components used in building interactive visualizations on the web.",
  "main": "dist/vue-charts.umd.js",
  "module": "dist/vue-charts.esm.js",
  "unpkg": "dist/vue-charts.min.js",
  "scripts": {
    "docs:dev": "node_modules/.bin/vuepress dev documentation/",
    "docs:build": "node_modules/.bin/vuepress build documentation/",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-charts.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-charts.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-charts.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.mitre.org:full-stack-interactive-visualizations/components.git"
  },
  "author": "Harout Boujakjian",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@vue/cli-service": "^4.5.11",
    "rollup": "^2.40.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-vue": "^6.0.0",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.8.0"
  },
  "dependencies": {
    "colorbrewer": "^1.3.0",
    "d3-array": "^2.11.0",
    "d3-axis": "^2.0.0",
    "d3-force": "^2.1.1",
    "d3-hierarchy": "^2.0.0",
    "d3-scale": "^3.2.3",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.0.0",
    "d3-transition": "^2.0.0",
    "leaflet": "^1.7.1"
  }
}
