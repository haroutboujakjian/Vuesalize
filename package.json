{
  "name": "vuesalize",
  "version": "0.0.34",
  "description": "Component library dedicated to simplifying interactive visualization building in Vue.js.",
  "private": false,
  "main": "dist/vuesalize.common.js",
  "unpkg": "dist/vuesalize.umd.min.js",
  "scripts": {
    "docs:dev": "node_modules/.bin/vuepress dev documentation_src/",
    "docs:build": "node_modules/.bin/vuepress build documentation_src/",
    "build": "vue-cli-service build --target lib --name vuesalize src/main.js && rm dist/demo.html"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/haroutboujakjian/Vuesalize"
  },
  "author": "Harout Boujakjian",
  "license": "MIT",
  "devDependencies": {
    "@vue/cli-service": "^4.5.12",
    "vuepress": "^1.8.0"
  },
  "dependencies": {
    "colorbrewer": "^1.3.0",
    "d3-array": "^2.11.0",
    "d3-axis": "^2.0.0",
    "d3-force": "^2.1.1",
    "d3-hierarchy": "^2.0.0",
    "d3-scale": "^3.2.3",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.0.0",
    "d3-transition": "^2.0.0",
    "d3-zoom": "^2.0.0",
    "vue": "^2.6.12"
  },
  "keywords": [
    "vue.js",
    "vue",
    "d3",
    "d3.js",
    "visualization",
    "data visualization",
    "component library",
    "ui library"
  ]
}
